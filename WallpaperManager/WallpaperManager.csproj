  <PropertyGroup>
    <PostBuildEvent>IF "$(ConfigurationName)" == "Release (deploy ready)" (
  SET WinRar=C:\Program Files\WinRAR\WinRAR.exe

  CD "$(OutDir)"
  REM Delete files we don't need for the release.
  DEL /S /Q "*.xml"
  DEL /S /Q "*.application"
  DEL /S /Q "*.pdb"
 
  REM Pack them all into a zip file and delete them.
  "%25WinRar%25" M -R -AFZIP "Wallpaper Manager.zip" "*.*"

  REM Sleep 1 second because WinRar does not release the file lock quickly enought sometimes.
  ping 127.0.0.1&gt;NUL

  REM And finialize it.
  "%25WinRar%25" K "Wallpaper Manager.zip" "*.*"
)</PostBuildEvent>
  </PropertyGroup>
